    <style name="TimePicker" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorAccent">@color/app_color</item>
        <item name="colorControlNormal">@color/red</item>
    </style>

    <style name="ProgressBar" parent="@style/Widget.AppCompat.ProgressBar.Horizontal">
        <item name="colorButtonNormal">#f50057</item>
        <item name="colorControlActivated">#33691e</item>
        <item name="colorControlHighlight">#ff6d00</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:progressDrawable">@drawable/custom_progressbar</item>
        <item name="android:minWidth">200dp</item>
        <item name="android:max">100</item>
    </style>

    <style name="SeekBar">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:minHeight">@dimen/dp15</item>
        <item name="android:minWidth">@dimen/dp15</item>
        <item name="android:maxHeight">@dimen/dp15</item>
        <item name="android:maxWidth">@dimen/dp15</item>
        <item name="android:progress">0</item>
    </style>

    <style name="switch_text_appearance" parent="@android:style/TextAppearance.Holo.Small">
